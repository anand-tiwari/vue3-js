{
  "name": "search-ui",
  "version": "1.0.0-1",
  "private": true,
  "author": "Anand Tiwari",
  "scripts": {
    "dev": "vue-cli-service serve --mode development",
    "dev-nomock": "vue-cli-service serve --mode staging",
    "build:prod": "vue-cli-service build",
    "build:stage": "vue-cli-service build --mode staging",
    "preview": "npm run build:stage && node build/index.js --mode staging",
    "lint": "vue-cli-service lint",
    "lint:fix": "eslint --ext .js,.vue src tests/unit --fix",
    "posttest": "node build/post-unit-test.js",
    "test:unit": "jest --clearCache && vue-cli-service test:unit",
    "test:report": "npm run test:unit && npm run posttest",
    "test:sonar-local": "npm run test:report && sonar-scanner",
    "package-release": "npm run build:prod && node build/package-release.js && node build/dockerize.js release",
    "package-snapshot": "npm run build:prod && node build/package-snapshot.js && node build/dockerize.js snapshot"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "axios-mock-adapter": "^1.18.2",
    "core-js": "^3.6.5",
    "register-service-worker": "^1.7.1",
    "vue": "^3.0.0-0",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-pwa": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0-0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^2.0.0-0",
    "archiver": "^5.0.2",
    "babel-eslint": "^10.1.0",
    "connect": "^3.7.0",
    "copy-webpack-plugin": "^6.2.0",
    "eslint": "^6.7.2",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^7.0.0-0",
    "http-proxy-middleware": "^1.0.5",
    "prettier": "^1.19.1",
    "replace-in-file": "^6.1.0",
    "serve-static": "^1.14.1",
    "typescript": "~3.9.3",
    "vue-jest": "^5.0.0-0"
  }
}
